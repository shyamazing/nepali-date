    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-plugin-import": "^2.12.0",
    "jest": "^23.3.0",
    "jest-babel": "^1.0.1",
    "rimraf": "^2.6.2"
  },
  "dependencies": {}
}
